@{
    Layout = "~/Views/Shared/_Layout_Set.cshtml";
}
@section CSS{

}

<div class="wrapper  animated fadeInUp">
    <div class="row">
        <div class="col-sm-12">
            <div class="ibox">
                <div class="ibox-content" ng-controller="BackgroundJobLogInfoController">
                    <form class="form-horizontal m-t" id="form" role="form">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Job名称：</label>
                            <div class="col-sm-8">
                                <input id="JobName" name="JobName" class="form-control" type="text" ng-model="model.JobName" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">执行时间：</label>
                            <div class="col-sm-8">
                                <input id="ExecutionTime" name="ExecutionTime" class="form-control" type="text" ng-model="model.ExecutionTime" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">执行持续时长：</label>
                            <div class="col-sm-8">
                                <input id="ExecutionDuration" name="ExecutionDuration" class="form-control" type="text" ng-model="model.ExecutionDuration" />
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-sm-3 control-label">日志创建时间：</label>
                            <div class="col-sm-8">
                                <input id="CreatedDateTime" name="CreatedDateTime" class="form-control" type="text" ng-model="model.CreatedDateTime" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">日志内容：</label>
                            <div class="col-sm-8">
                                <textarea id="RunLog" name="RunLog" class="form-control" ng-model="model.RunLog" style="width: 450px; height: 200px;"></textarea>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section JS{
    <script src="/Scripts/controllers/backgroundjoblog.controller.js"></script>
}
